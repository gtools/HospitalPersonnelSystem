            </div>
            <div class="form-group">
                <label asp-for="EmpName" class="control-label"></label>
                <input asp-for="EmpName" class="form-control" data-spell="name" />
                <span asp-validation-for="EmpName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Spell" class="control-label"></label>
                <input asp-for="Spell" class="form-control" data-spell="spell" readonly />
                <span asp-validation-for="Spell" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="DeptCode" class="control-label"></label>
                <select asp-for="DeptCode" class="form-control" asp-items="ViewBag.DeptCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="OldName" class="control-label"></label>
                <input asp-for="OldName" class="form-control" />
                <span asp-validation-for="OldName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="GenderCode" class="control-label"></label>

            <div class="form-group">
                <label asp-for="WorkAge" class="control-label"></label>
                <input asp-for="WorkAge" class="form-control" />
                <span asp-validation-for="WorkAge" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="HospitalAge" class="control-label"></label>
                <input asp-for="HospitalAge" class="form-control" />
                <span asp-validation-for="HospitalAge" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Practice" class="control-label"></label>
                <input asp-for="Practice" class="form-control" />
                <span asp-validation-for="Practice" class="text-danger"></span>
            </div>

            </div>
            <div class="form-group">
                <label asp-for="NationCode" class="control-label"></label>
                <select asp-for="NationCode" class="form-control" asp-items="ViewBag.NationCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="Age" class="control-label"></label>
                <input asp-for="Age" class="form-control" />
                <span asp-validation-for="Age" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="PoliticalCode" class="control-label"></label>
                <select asp-for="PoliticalCode" class="form-control" asp-items="ViewBag.PoliticalCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="PoliticalDate" class="control-label"></label>
                <input asp-for="PoliticalDate" class="form-control" />
                <span asp-validation-for="PoliticalDate" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="IdentityCard" class="control-label"></label>
                <input asp-for="IdentityCard" class="form-control" />
                <span asp-validation-for="IdentityCard" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="ProfessionTitleTypeCode" class="control-label"></label>
                <select asp-for="ProfessionTitleTypeCode" class="form-control" asp-items="ViewBag.ProfessionTitleTypeCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProfessionTitleLevelCode" class="control-label"></label>
                <select asp-for="ProfessionTitleLevelCode" class="form-control" asp-items="ViewBag.ProfessionTitleLevelCode"></select>
            </div>
            <div class="form-group">
                <label asp-for="ProfessionTitleCode" class="control-label"></label>
                <select asp-for="ProfessionTitleCode" class="form-control" asp-items="ViewBag.ProfessionTitleCode"></select>

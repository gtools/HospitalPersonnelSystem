@model HospitalPersonnelSystem.Models.SysEmp

@{
    ViewData["Title"] = "人员管理 - 创建";
    ViewData["Controller"] = "SysEmp";
}

<h1>@ViewData["Title"]</h1>

<div>
    <a asp-action="Index" data-toggle="tooltip" data-placement="left" title="返回"><i class="fa fa-arrow-alt-circle-left fa-lg"></i></a>
</div>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="EmpCode" class="control-label"></label>
                <input asp-for="EmpCode" class="form-control" />

            </div>
            <div class="form-group">
                <label asp-for="Remark" class="control-label"></label>
                <input asp-for="Remark" class="form-control" />
                <span asp-validation-for="Remark" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CreateEmp" class="control-label"></label>
                <input asp-for="CreateEmp" class="form-control" />
                <span asp-validation-for="CreateEmp" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="保&nbsp;&nbsp;存" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

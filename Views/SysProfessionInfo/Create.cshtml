@model HospitalPersonnelSystem.Models.SysProfessionInfo

@{
    ViewData["Title"] = "职称评定 - 创建";
    ViewData["Controller"] = "SysProfessionInfo";
}

<h1>@ViewData["Title"]</h1>

<div>
    <a asp-action="Index" data-toggle="tooltip" data-placement="left" title="返回"><i class="fa fa-arrow-alt-circle-left fa-lg"></i></a>
</div>
<hr />
<div class="row">
    <div class="col-md-6">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="EmpCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <gt-select asp-for="EmpCode" asp-items="ViewBag.EmpCode" is-value="true"></gt-select>
                    <div class="input-group-prepend">
                        <label asp-for="TypeCode" class="input-group-text"></label>
                    </div>
                    <input asp-for="TypeCode" class="form-control" />

                </div>
                <span asp-validation-for="EmpCode" class="text-danger"></span>
                <span asp-validation-for="TypeCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionRegisterCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionRegisterCode" class="form-control" asp-items="ViewBag.ProfessionRegisterCode"></select>
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionExtentCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionExtentCode" class="form-control" asp-items="ViewBag.ProfessionExtentCode"></select>
                </div>
                <span asp-validation-for="ProfessionExtentCode" class="text-danger"></span>
                <span asp-validation-for="ProfessionRegisterCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionTitleTypeCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionTitleTypeCode" class="form-control" asp-items="ViewBag.ProfessionTitleTypeCode"></select>
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionTitleLevelCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionTitleLevelCode" class="form-control" asp-items="ViewBag.ProfessionTitleLevelCode"></select>
                </div>
                <span asp-validation-for="ProfessionTitleTypeCode" class="text-danger"></span>
                <span asp-validation-for="ProfessionTitleLevelCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionTitleCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionTitleCode" class="form-control" asp-items="ViewBag.ProfessionTitleCode"></select>
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionTypeCode" class="input-group-text font-weight-bold"></label>
                    </div>
                    <select asp-for="ProfessionTypeCode" class="form-control" asp-items="ViewBag.ProfessionTypeCode"></select>
                </div>
                <span asp-validation-for="ProfessionTitleCode" class="text-danger"></span>
                <span asp-validation-for="ProfessionTypeCode" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="EngageDate" class="input-group-text font-weight-bold"></label>
                    </div>
                    <input asp-for="EngageDate" class="form-control" />
                    <div class="input-group-prepend">
                        <label asp-for="ProfessionalName" class="input-group-text"></label>
                    </div>
                    <input asp-for="ProfessionalName" class="form-control" />
                </div>
                <span asp-validation-for="EngageDate" class="text-danger"></span>
                <span asp-validation-for="ProfessionalName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="No1" class="input-group-text"></label>
                    </div>
                    <input asp-for="No1" class="form-control" />
                </div>
                <span asp-validation-for="No1" class="text-danger"></span>
            </div>

            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="No2" class="input-group-text"></label>
                    </div>
                    <input asp-for="No2" class="form-control" />
                </div>
                <span asp-validation-for="No2" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="No3" class="input-group-text"></label>
                    </div>
                    <input asp-for="No3" class="form-control" />
                </div>
                <span asp-validation-for="No3" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="No4" class="input-group-text"></label>
                    </div>
                    <input asp-for="No4" class="form-control" />
                </div>
                <span asp-validation-for="No4" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="input-group-prepend">
                        <label asp-for="No5" class="input-group-text"></label>
                    </div>
                    <input asp-for="No5" class="form-control" />
                </div>
                <span asp-validation-for="No5" class="text-danger"></span>
            </div>
            <div class="form-group">
                <div class="input-group">
                    <div class="custom-control custom-switch">
                        <input asp-for="IsEngage" class="custom-control-input" />
                        <label asp-for="IsEngage" class="custom-control-label"></label>
                    </div>
                </div>
                <span asp-validation-for="IsEngage" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="保&nbsp;&nbsp;存" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
